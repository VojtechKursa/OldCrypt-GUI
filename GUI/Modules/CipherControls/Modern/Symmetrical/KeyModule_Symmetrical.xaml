<UserControl x:Class="OldCrypt.GUI.GUI.Modules.CipherControls.KeyModule_Symmetrical"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OldCrypt.GUI.GUI.Modules.CipherControls"
             mc:Ignorable="d" 
             d:DesignHeight="37" d:DesignWidth="260">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="L_label" Content="Key:" VerticalAlignment="Top" FontSize="18" VerticalContentAlignment="Center" Height="37"/>
        <Button x:Name="B_load" Content="Load" Margin="5,5,5,0" FontSize="14" Grid.Column="1" Click="B_load_Click" Height="27" VerticalAlignment="Top"/>
        <Button x:Name="B_save" Content="Save" Margin="5,5,5,0" VerticalAlignment="Top" FontSize="14" Height="27" Grid.Column="2" Click="B_save_Click"/>
        <Button x:Name="B_show" Content="Show" Margin="5,5,5,0" VerticalAlignment="Top" FontSize="14" Height="27" Grid.Column="3" Click="B_show_Click"/>
        <Button x:Name="B_generate" Content="Generate" Margin="5,5,5,0" VerticalAlignment="Top" FontSize="14" Height="27" Grid.Column="4" Click="B_generate_Click"/>
    </Grid>
</UserControl>
